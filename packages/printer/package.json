{
  "name": "@react-thermal-printer/printer",
  "type": "module",
  "version": "0.18.0",
  "description": "Printer interface provides ESC/POS commands.",
  "sideEffects": false,
  "license": "MIT",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "attw": "yarn pack && attw package.tgz",
    "prepack": "yarn build",
    "build": "tsdown"
  },
  "dependencies": {
    "@react-thermal-printer/image": "workspace:^"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "iconv-lite": "^0.6.3",
    "tsdown": "0.14.1"
  }
}
