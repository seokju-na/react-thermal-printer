{
  "name": "@react-thermal-printer/printer",
  "version": "0.16.1",
  "description": "Printer interface provides ESC/POS commands.",
  "sideEffects": false,
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.mjs",
    "import": "./dist/index.mjs",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "tsup"
  },
  "dependencies": {
    "@react-thermal-printer/image": "workspace:^"
  },
  "devDependencies": {
    "iconv-lite": "^0.6.3",
    "tsup": "^8.3.0"
  }
}
