{
  "name": "@react-thermal-printer/image",
  "version": "0.12.0",
  "description": "Image interface for printing",
  "sideEffects": false,
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.mjs",
    "import": "./dist/index.mjs",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "tsup"
  },
  "dependencies": {
    "floyd-steinberg": "^1.0.6"
  },
  "devDependencies": {
    "@types/floyd-steinberg": "^1.0.0",
    "tsup": "^8.3.0"
  }
}
