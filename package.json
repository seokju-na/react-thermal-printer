{
  "name": "react-thermal-printer-sources",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.9.3",
  "workspaces": [
    "packages/*",
    "example",
    "docs"
  ],
  "scripts": {
    "check": "biome check",
    "check:fix": "yarn check --unsafe --write",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit --incremental",
    "attw": "yarn workspaces foreach -Ap --include='packages/*' run attw",
    "test": "vitest run"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jsdom": "26.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "5.9.2",
    "vitest": "3.0.6"
  }
}
